{
  "name": "Nodedit",
  "version": "0.7.3",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "description": "A thin-client web-based interactive develoment environment for interacting with remote file systems",
  "homepage_url": "http://nodedit.com",
  "icons": {
    "16": "logos/nodedit_16.png",
    "48": "logos/nodedit_48.png",
    "128": "logos/nodedit_128.png"
  },
  "permissions": [
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}